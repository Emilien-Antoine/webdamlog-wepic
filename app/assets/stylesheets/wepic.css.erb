/************************************************************************************************************************/
/*Wepic version of fancybox for outer boxes*/
#wepicbox-loading {
  position: fixed;
  top: 50%;
  left: 50%;
  width: 40px;
  height: 40px;
  margin-top: -20px;
  margin-left: -20px;
  cursor: pointer;
  overflow: hidden;
  z-index: 1104;
  display: none;
}

#wepicbox-loading div {
  position: absolute;
  top: 0;
  left: 0;
  width: 40px;
  height: 480px;
  background-image: url(<%= asset_path 'fancybox.png' %>);
}

#wepicbox-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1100;
  display: none;
}

#wepicbox-tmp {
  padding: 0;
  margin: 0;
  border: 0;
  overflow: auto;
  display: none;
}

#wepicbox-wrap {
  position: absolute;
  top: 0;
  left: 0;
  padding: 20px;
  z-index: 1101;
  outline: none;
  display: none;
}

#wepicbox-outer {
  position: relative;
  width: 100%;
  height: 100%;
  background: #fff;
}

#wepicbox-content {
  width: 0;
  height: 0;
  padding: 0;
  outline: none;
  position: relative;
  overflow: hidden;
  z-index: 1102;
  border: 0px solid #fff;
}

#wepicbox-hide-sel-frame {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: transparent;
  z-index: 1101;
}

#wepicbox-close {
  position: absolute;
  top: -15px;
  right: -15px;
  width: 30px;
  height: 30px;
  background: transparent url(<%= asset_path 'fancybox.png' %>) -40px 0px;
  cursor: pointer;
  z-index: 1103;
  display: none;
}

#wepicbox-error {
  color: #444;
  font: normal 12px/20px Arial;
  padding: 14px;
  margin: 0;
}

#wepicbox-img {
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
  border: none;
  outline: none;
  line-height: 0;
  vertical-align: top;
}

#wepicbox-frame {
  width: 100%;
  height: 100%;
  border: none;
  display: block;
}

#wepicbox-left, #wepicbox-right {
  position: absolute;
  bottom: 0px;
  height: 100%;
  width: 35%;
  cursor: pointer;
  outline: none;
  background: transparent url(<%= asset_path 'blank.gif' %>);
  z-index: 1102;
  display: none;
}

#wepicbox-left {
  left: 0px;
}

#wepicbox-right {
  right: 0px;
}

#wepicbox-left-ico, #wepicbox-right-ico {
  position: absolute;
  top: 50%;
  left: -9999px;
  width: 30px;
  height: 30px;
  margin-top: -15px;
  cursor: pointer;
  z-index: 1102;
  display: block;
}

#wepicbox-left-ico {
  background-image: url(<%= asset_path 'fancybox.png' %>);
  background-position: -40px -30px;
}

#wepicbox-right-ico {
  background-image: url(<%= asset_path 'fancybox.png' %>);
  background-position: -40px -60px;
}

#wepicbox-left:hover, #wepicbox-right:hover {
  visibility: visible; /* IE6 */
}

#wepicbox-left:hover span {
  left: 20px;
}

#wepicbox-right:hover span {
  left: auto;
  right: 20px;
}

.wepicbox-bg {
  position: absolute;
  padding: 0;
  margin: 0;
  border: 0;
  width: 20px;
  height: 20px;
  z-index: 1001;
}

#wepicbox-bg-n {
  top: -20px;
  left: 0;
  width: 100%;
  background-image: url(<%= asset_path 'fancybox-x.png' %>);
}

#wepicbox-bg-ne {
  top: -20px;
  right: -20px;
  background-image: url(<%= asset_path 'fancybox.png' %>);
  background-position: -40px -162px;
}

#wepicbox-bg-e {
  top: 0;
  right: -20px;
  height: 100%;
  background-image: url(<%= asset_path 'fancybox-y.png' %>);
  background-position: -20px 0px;
}

#wepicbox-bg-se {
  bottom: -20px;
  right: -20px;
  background-image: url(<%= asset_path 'fancybox.png' %>);
  background-position: -40px -182px; 
}

#wepicbox-bg-s {
  bottom: -20px;
  left: 0;
  width: 100%;
  background-image: url(<%= asset_path 'fancybox-x.png' %>);
  background-position: 0px -20px;
}

#wepicbox-bg-sw {
  bottom: -20px;
  left: -20px;
  background-image: url(<%= asset_path 'fancybox.png' %>);
  background-position: -40px -142px;
}

#wepicbox-bg-w {
  top: 0;
  left: -20px;
  height: 100%;
  background-image: url(<%= asset_path 'fancybox-y.png' %>);
}

#wepicbox-bg-nw {
  top: -20px;
  left: -20px;
  background-image: url(<%= asset_path 'fancybox.png' %>);
  background-position: -40px -122px;
}

#wepicbox-title {
  font-family: Helvetica;
  font-size: 12px;
  z-index: 1102;
}

.wepicbox-title-inside {
  padding-bottom: 10px;
  text-align: center;
  color: #333;
  background: #fff;
  position: relative;
}

.wepicbox-title-outside {
  padding-top: 10px;
  color: #fff;
}

.wepicbox-title-over {
  position: absolute;
  bottom: 0;
  left: 0;
  color: #FFF;
  text-align: left;
}

#wepicbox-title-over {
  padding: 10px;
  background-image: url(<%= asset_path 'fancy_title_over.png' %>);
  display: block;
}

.wepicbox-title-float {
  position: absolute;
  left: 0;
  bottom: -20px;
  height: 32px;
}

#wepicbox-title-float-wrap {
  border: none;
  border-collapse: collapse;
  width: auto;
}

#wepicbox-title-float-wrap td {
  border: none;
  white-space: nowrap;
}

#wepicbox-title-float-left {
  padding: 0 0 0 15px;
  background: url(<%= asset_path 'fancybox.png' %>) -40px -90px no-repeat;
}

#wepicbox-title-float-main {
  color: #FFF;
  line-height: 29px;
  font-weight: bold;
  padding: 0 0 3px 0;
  background: url(<%= asset_path 'fancybox-x.png' %>) 0px -40px;
}

#wepicbox-title-float-right {
  padding: 0 0 0 15px;
  background: url(<%= asset_path 'fancybox.png' %>) -55px -90px no-repeat;
}

/* IE6 */

.wepicbox-ie6 #wepicbox-close { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_close.png' %>", sizingMethod='scale'); }

.wepicbox-ie6 #wepicbox-left-ico { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_nav_left.png' %>", sizingMethod='scale'); }
.wepicbox-ie6 #wepicbox-right-ico { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_nav_right.png' %>", sizingMethod='scale'); }

.wepicbox-ie6 #wepicbox-title-over { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_title_over.png' %>", sizingMethod='scale'); zoom: 1; }
.wepicbox-ie6 #wepicbox-title-float-left { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_title_left.png' %>", sizingMethod='scale'); }
.wepicbox-ie6 #wepicbox-title-float-main { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_title_main.png' %>", sizingMethod='scale'); }
.wepicbox-ie6 #wepicbox-title-float-right { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_title_right.png' %>", sizingMethod='scale'); }

.wepicbox-ie6 #wepicbox-bg-w, .wepicbox-ie6 #wepicbox-bg-e, .wepicbox-ie6 #wepicbox-left, .wepicbox-ie6 #wepicbox-right, #wepicbox-hide-sel-frame {
  height: expression(this.parentNode.clientHeight + "px");
}

#wepicbox-loading.wepicbox-ie6 {
  position: absolute; margin-top: 0;
  top: expression( (-20 + (document.documentElement.clientHeight ? document.documentElement.clientHeight/2 : document.body.clientHeight/2 ) + ( ignoreMe = document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop )) + 'px');
}

#wepicbox-loading.wepicbox-ie6 div  { background: transparent; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_loading.png' %>", sizingMethod='scale'); }

/* IE6, IE7, IE8 */

.wepicbox-ie .wepicbox-bg { background: transparent !important; }

.wepicbox-ie #wepicbox-bg-n { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_n.png' %>", sizingMethod='scale'); }
.wepicbox-ie #wepicbox-bg-ne { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_ne.png' %>", sizingMethod='scale'); }
.wepicbox-ie #wepicbox-bg-e { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_e.png' %>", sizingMethod='scale'); }
.wepicbox-ie #wepicbox-bg-se { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_se.png' %>", sizingMethod='scale'); }
.wepicbox-ie #wepicbox-bg-s { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_s.png' %>", sizingMethod='scale'); }
.wepicbox-ie #wepicbox-bg-sw { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_sw.png' %>", sizingMethod='scale'); }
.wepicbox-ie #wepicbox-bg-w { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_w.png' %>", sizingMethod='scale'); }
.wepicbox-ie #wepicbox-bg-nw { filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="<%= asset_path 'fancy_shadow_nw.png' %>", sizingMethod='scale'); }